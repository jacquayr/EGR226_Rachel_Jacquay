#include "msp.h"
#include "stdio.h"

void TimerA0_Init(void)

void main(void)
{
    WDT_A->CTL = WDT_A_CTL_PW | WDT_A_CTL_HOLD;		// stop watchdog timer
    TimerA0_Init();

    while(1) {

    }
}

/*--------------------------------------------------------------
 * Function:
 *
 * Description:
 *
 * Inputs:          none
 *
 * Outputs:         none
 *-------------------------------------------------------------*/
void TimerA0_Init(void) {
    P2->SEL0 |= BIT4;   // P2.4 set TA0.1
    P2->SEL0 &= ~BIT4;
    P2->DIR |= BIT4;


}
